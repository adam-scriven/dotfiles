text/html; w3m -I %{charset} -T text/html; copiousoutput;
image/*; eog %s
application/pdf; evince %s
application/vnd.openxmlformats-officedocument.wordprocessingml.document; soffice --nologo --writer %s > /dev/null 2>&1
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; soffice --nologo --calc %s > /dev/null 2>&1
